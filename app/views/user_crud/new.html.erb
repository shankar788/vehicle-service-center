<div>
<%= form_with model: @user,url:'/user_crud' ,method: :post do |form| %>
<div>
    <%= form.label :Role%><br>
    <%= form.text_field :user_rule%>
</div>


<div>
    <%= form.label :email%><br>
    <%= form.email_field :email%>
</div>

<div>
    <%= form.label :Password%><br>
    <%= form.password_field :password%>
</div>
  <div>
    <%= form.submit %>
  </div>
<% end %>






 </div>